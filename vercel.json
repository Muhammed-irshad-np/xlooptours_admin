{
  "rewrites": [
    {
      "source": "/",
      "destination": "/home.html"
    },
    {
      "source": "/admin",
      "destination": "/index.html"
    },
    {
      "source": "/admin/",
      "destination": "/index.html"
    },
    {
      "source": "/admin/((?!.*\\.).*)",
      "destination": "/index.html"
    },
    {
      "source": "/admin/(.*)",
      "destination": "/$1"
    }
  ]
}